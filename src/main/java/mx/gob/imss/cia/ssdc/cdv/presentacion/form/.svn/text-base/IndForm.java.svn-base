package mx.gob.imss.cia.ssdc.cdv.presentacion.form;

import java.util.ArrayList;
import java.util.List;

import javax.faces.model.SelectItem;

import org.springframework.stereotype.Component;

/**
 * @author Gamaliel
 * 
 */
@Component
public class IndForm {
	private String tipoReporte = "0";
	private String delegacion;
	private String distProblematica;
	private String urlImagen;
	private String cobertura;
	private List<SelectItem> categorias = new ArrayList<SelectItem>();
	private List<SelectItem> categoriasSeleccionadas = 
		new ArrayList<SelectItem>();
	private List<SelectItem> listaCobertura = new ArrayList<SelectItem>();
	private List<SelectItem> listaDelegaciones = new ArrayList<SelectItem>();

	/**
	 * @return the tipoReporte
	 */
	public String getTipoReporte() {
		return tipoReporte;
	}

	/**
	 * @param tipoReporte
	 *            the tipoReporte to set
	 */
	public void setTipoReporte(String tipoReporte) {
		this.tipoReporte = tipoReporte;
	}

	/**
	 * @return the delegacion
	 */
	public String getDelegacion() {
		return delegacion;
	}

	/**
	 * @param delegacion
	 *            the delegacion to set
	 */
	public void setDelegacion(String delegacion) {
		this.delegacion = delegacion;
	}

	/**
	 * @return the distProblematica
	 */
	public String getDistProblematica() {
		return distProblematica;
	}

	/**
	 * @param distProblematica
	 *            the distProblematica to set
	 */
	public void setDistProblematica(String distProblematica) {
		this.distProblematica = distProblematica;
	}

	/**
	 * @return the urlImagen
	 */
	public String getUrlImagen() {
		return urlImagen;
	}

	/**
	 * @param urlImagen
	 *            the urlImagen to set
	 */
	public void setUrlImagen(String urlImagen) {
		this.urlImagen = urlImagen;
	}

	/**
	 * @return the cobertura
	 */
	public String getCobertura() {
		return cobertura;
	}

	/**
	 * @param cobertura
	 *            the cobertura to set
	 */
	public void setCobertura(String cobertura) {
		this.cobertura = cobertura;
	}

	/**
	 * @return the categorias
	 */
	public List<SelectItem> getCategorias() {
		return categorias;
	}

	/**
	 * @param categorias
	 *            the categorias to set
	 */
	public void setCategorias(List<SelectItem> categorias) {
		this.categorias = categorias;
	}

	/**
	 * @return the categoriasSeleccionadas
	 */
	public List<SelectItem> getCategoriasSeleccionadas() {
		return categoriasSeleccionadas;
	}

	/**
	 * @param categoriasSeleccionadas
	 *            the categoriasSeleccionadas to set
	 */
	public void setCategoriasSeleccionadas(
			List<SelectItem> categoriasSeleccionadas) {
		this.categoriasSeleccionadas = categoriasSeleccionadas;
	}

	/**
	 * @return the listaCobertura
	 */
	public List<SelectItem> getListaCobertura() {
		return listaCobertura;
	}

	/**
	 * @param listaCobertura
	 *            the listaCobertura to set
	 */
	public void setListaCobertura(List<SelectItem> listaCobertura) {
		this.listaCobertura = listaCobertura;
	}

	/**
	 * @return the listaDelegaciones
	 */
	public List<SelectItem> getListaDelegaciones() {
		return listaDelegaciones;
	}

	/**
	 * @param listaDelegaciones
	 *            the listaDelegaciones to set
	 */
	public void setListaDelegaciones(List<SelectItem> listaDelegaciones) {
		this.listaDelegaciones = listaDelegaciones;
	}

}
